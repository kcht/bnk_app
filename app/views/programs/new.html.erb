<h1>New BNK program</h1>
<%= form_for :program, url: programs_path do |f| %>
    <p>
      <%= f.label :number %><br>
      <%= f.text_field :number %>
    </p>

    <p>
      <%= f.label :date_aired %><br>
      <%= f.date_field :date %>
    </p>
    <p>
      <%= f.label :name %><br>
      <%= f.text_field :name %>
    </p>

    <p>
      <%= f.label :description %><br>
      <%= f.text_area :description %>
    </p>

    <p>
      <%= f.submit %>
    </p>
<% end %>